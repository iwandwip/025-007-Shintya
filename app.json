{
  "expo": {
    "name": "Shintya App",
    "slug": "shintya-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-packet.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-packet.png",
      "resizeMode": "contain",
      "backgroundColor": "#F50057"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-packet.png",
        "backgroundColor": "#F50057"
      },
      "package": "com.haikal.haikalapp"
    },
    "web": {
      "favicon": "./assets/icon-packet.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "firebase-auth-template",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "878acea5-1787-4765-a329-78bb9b93b718"
      }
    }
  }
}
