{
  "expo": {
    "name": "Shintya App",
    "slug": "shintya-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-packet.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-packet.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-packet.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icon-packet.png"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-router"
    ],
    "scheme": "smart-packet-box",
    "experiments": {
      "typedRoutes": false
    }
  }
}
